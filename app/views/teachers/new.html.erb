<h1>Welcome New Teacher!</h1>

<h3>Please create your profile:</h3>

<%= form_for(@teacher) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %><br><br>

<%= f.label :email %>
<%= f.email_field :email %><br><br>

<%= f.label :password %>
<%= f.password_field :password %><br><br>

<%= f.label :subject %>
<%= f.text_field :subject %><br><br>

<%= f.submit "Create Teacher" %>

<% end %>
